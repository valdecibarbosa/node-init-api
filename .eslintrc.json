{
    "env": {
        "es2020": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
    "linebreak-style": 0
    },
    "settings": {
        "import/core-modules": ["@expo/vector-icons"],
        "import/extensions": [".js", ".ts"],
        "import/parsers": {
        "@typescript-eslint/parser": [".ts"]
        },
        "import/resolver": {
        "typescript": {},
        "node": {
            "extensions": [".js", ".ts"]
        }
        }
    }
    
    
}
